{
  "source": {
    "minio_source": {
        "bucket": "scamp",
        "data": "dev355_m.csv"
    }
  },
  "out": {
    "grafana": {},
    "kafka": {}
  },
  "operators": {
    "scaler": {
      "MinMaxScaler": {
        "copy": 0,
        "clip": 0
      }
    },
    "cycle_detect": {
      "pattern": "df_pattern_d355.csv",
      "max_distance": 10.0,
      "mark": "True",
      "delta_bias": 10.0
    },

    "cluster": {
      "HDSCAN": {
        "min_cluster_size": 5,
        "min_samples": 5,
        "metric": "dtw",
        "algorithm": "best",
        "leaf_size": 30,
        "p": null
      },
      "mark": "True"
    },
    "anomaly": {
      "IsolationForest": {
        "n_estimators": 100,
        "max_samples": "auto",
        "contamination": "auto",
        "max_features": 1.0,
        "bootstrap": "False",
        "n_jobs": -1,
        "behaviour": "new",
        "random_state": null,
        "verbose": 0,
        "warm_start": "False"
      },
      "mark": "True"
    }
  }
}
